{
  "name": "futureui",
  "version": "0.0.1",
  "author": {
    "name": "Dev By RayRay",
    "email": "raymon@byrayray.dev"
  },
  "description": "Future UI a StencilJS Component Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/futureui/futureui.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/devbyray/future-ui.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "prebuild": "npx tailwindcss -i ./src/styles/tailwind.css -o ./www/build/tailwind.css",
    "build": "stencil build --docs",
    "prebuild:prod": "npx tailwindcss -i ./src/styles/tailwind.css -o ./www/build/tailwind.css",
    "build:prod": "stencil build --prod",
    "prestart": "npx tailwindcss -i ./src/styles/tailwind.css -o ./www/build/tailwind.css",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "pregenerate": "npx tailwindcss -i ./src/styles/tailwind.css -o ./www/build/tailwind.css",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@stencil/core": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "postcss-import": "^15.1.0"
  },
  "devDependencies": {
    "@stencil/postcss": "^2.1.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "21.1.1",
    "stencil-tailwind-plugin": "^1.8.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
